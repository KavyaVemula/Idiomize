<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homebg"
    android:orientation="vertical" >
    
    

    <Button
        android:id="@+id/bmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Back to Levels" />

    <Button
        android:id="@+id/test1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/id2"
        android:layout_centerVertical="true"
        android:text="learn" />

    <Button
        android:id="@+id/id3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/id2"
        android:layout_alignBottom="@+id/id2"
        android:layout_marginLeft="23dp"
        android:layout_toRightOf="@+id/test1"
        android:text="3" />

    <Button
        android:id="@+id/id1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/test1"
        android:layout_alignLeft="@+id/id4"
        android:layout_marginBottom="14dp"
        android:text="1" />

    <Button
        android:id="@+id/id2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/id1"
        android:layout_alignBottom="@+id/id1"
        android:layout_centerHorizontal="true"
        android:text="2" />

    <Button
        android:id="@+id/test2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bmenu"
        android:layout_alignLeft="@+id/id5"
        android:layout_marginBottom="19dp"
        android:text="learn" />

    <Button
        android:id="@+id/id4"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/id5"
        android:layout_alignBottom="@+id/id5"
        android:layout_marginRight="18dp"
        android:layout_toLeftOf="@+id/test1"
        android:text="4" />

    <Button
        android:id="@+id/id6"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/id3"
        android:layout_below="@+id/test1"
        android:layout_marginTop="16dp"
        android:text="6" />

    <Button
        android:id="@+id/id5"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/id6"
        android:layout_alignBottom="@+id/id6"
        android:layout_toLeftOf="@+id/id3"
        android:text="5" />

    <ImageView
        android:id="@+id/mini"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/mini" />

</RelativeLayout>
